.events {
  list-style: none;
  width: 100%;
  height: 100%;
  // background: red;
  margin: 0;
  padding: 0;
  font-size: 5px;
}
.events .ant-badge-status {
  overflow: hidden;
  white-space: nowrap;
  width: 100%;
  text-overflow: ellipsis;
  font-size: 10px;
}
.notes-month {
  text-align: center;
  font-size: 28px;
  background: olive;
}
.notes-month section {
  font-size: 28px;
}
//   详情modal
.detailContainer{
  width: 100%;
  // background: red;
}
.detailContainer>p{
  width: 100%;
}
.detailContainer>p>span{
  display: inline-block;
  margin: 5px 0;
}
.detailContainer>p>span:nth-of-type(1){
  width: 25%;
  text-align: right;
  color: black;
  font-size: 14px;
  font-weight: 700;
}
.detailContainer>p>span:nth-of-type(2){
  width: 75%;
  font-size: 14px;
}